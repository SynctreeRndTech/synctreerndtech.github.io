{
    "title": "Welcome to SyncTree!",
    "discription": "안녕하세요! SyncTree 가이드북입니다.",
    "plugins": [
        "collapsible-menu",
        "toggle-chapters",
        "insert-logo",
        "custom-favicon",
        "custom-js-css",
        "-sharing"
    ],
    "pluginsConfig": {
        "insert-logo": {
            "url": "/logo/LOGO_Synctree_GNB_M.svg",
            "style": "background: none; max-height: 32px; min-height: 32px"
        },
        "favicon": "/logo/favicon_sth_16.png",
        "custom-js-css": {
            "js": ["./js/custom.js"],
            "css": ["./styles/custom.css"]
        }
    },
    "styles": {
        "website": "/styles/website.css",
        "pdf": "/styles/pdf.css"
    },

    "pdf": {
        "margin": {
            "right": 34,
            "left": 34,
            "top": 34,
            "bottom": 38
        }
    },
    "variables": {
        "iframe": "show",
        "not-iframe": "noShow"
    }
